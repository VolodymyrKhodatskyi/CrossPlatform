@model IEnumerable<Lab6.Models.Customers>

<h1>Customers List</h1>

<table class="table">
    <thead>
    <tr>
            <th>CustomerName</th>
            <th>CustomerEmail</th>
            <th>CustomerAddress</th>
            <th>CommercialOrDomestic</th>
            <th>OtherCustomerDetails</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var customer in Model)
    {
        <tr>
                <td>@customer.CustomerName</td>
                <td>@customer.CustomerEmail</td>
                <td>@customer.CustomerAddress</td>
                <td>@customer.CommercialOrDomestic</td>
                <td>@customer.OtherCustomerDetails</td>
                
        </tr>
    }
    </tbody>
</table>